# AI Company Discovery Configuration
AI_DISCOVERY_ENABLED=true
AI_DISCOVERY_TIMEOUT=15000
AI_DISCOVERY_MAX_COMPANIES=50
AI_DISCOVERY_CACHE_TTL=3600000
AI_DISCOVERY_CONCURRENT_REQUESTS=5

# AI Job Search Enhancement Configuration
AI_JOB_ENHANCEMENT_ENABLED=true
AI_MATCHING_THRESHOLD=0.6
AI_SKILL_EXTRACTION_ENABLED=true
AI_INDUSTRY_EXPANSION_ENABLED=true

# Search Engine Configuration
SEARCH_ENGINE_TIMEOUT=10000
SEARCH_ENGINE_MAX_RESULTS=20
SEARCH_ENGINE_USER_AGENT="Mozilla/5.0 (compatible; NairaGigJobCrawler/1.0)"

# Company Discovery Sources
ENABLE_SEARCH_ENGINE_DISCOVERY=true
ENABLE_BUSINESS_DIRECTORY_DISCOVERY=true
ENABLE_LINKEDIN_DISCOVERY=true
ENABLE_CRUNCHBASE_DISCOVERY=true

# Crawler Performance Settings
CRAWLER_CONCURRENT_REQUESTS=10
CRAWLER_REQUEST_DELAY=1000
CRAWLER_MAX_RETRIES=3
CRAWLER_CACHE_ENABLED=true
CRAWLER_CACHE_TTL=1800000

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST_SIZE=10

# Logging
AI_SERVICE_LOG_LEVEL=info
CRAWLER_LOG_LEVEL=info